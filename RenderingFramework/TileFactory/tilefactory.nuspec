<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>TileFactory</id>
    <version>$version$</version>
    <authors>John Maloney</authors>
    <owners>maloney</owners>

    <projectUrl>https://github.com/johnmaloney/Geospatial-Information-System/tree/master/RenderingFramework/TileFactory</projectUrl>
    <!-- 
            If true, this value prompts the user to accept the license when
            installing the package. 
        -->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <!-- 
            The description can be used in package manager UI. Note that the
            nuget.org gallery uses information you add in the portal. 
        -->
    <description>Works on converting, generating and displaying vector tiles.</description>

    <!-- Copyright information -->
    <copyright>Copyright ©2019 Aetos Consulting</copyright>

    <!-- Tags appear in the gallery and can be used for tag searches -->
    <tags>gis vector tiles</tags>

    <!-- Dependencies are automatically installed when the package is installed -->
    <dependencies>
      <dependency id="GeoJSON.Net" version="1.1.*" />
      <dependency id="Google.Protobuf" version="3.8.*" />
    </dependencies>
  </metadata>

  <!-- A readme.txt to display when the package is installed 
  <files>
    <file src="readme.txt" target="" />
  </files>-->
</package>
