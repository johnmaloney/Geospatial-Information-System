    <!-- Dependencies are automatically installed when the package is installed -->
    <dependencies>
      <dependency id="Microsoft.Azure.ServiceBus" version="4.0.0" />
      <dependency id="Universal.Contracts" version="90.0.0" />
    </dependencies>
  </metadata>

  <!-- A readme.txt to display when the package is installed 
  <files>
    <file src="readme.txt" target="" />
  </files>-->
  </package>
