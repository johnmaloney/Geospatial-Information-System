<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Universal.Contracts</id>
    <version>$version$</version>
    <authors>John Maloney</authors>
    <owners>maloney</owners>

    <projectUrl>https://github.com/johnmaloney/PSU/tree/master/Universal.Contracts</projectUrl>
    <!-- 
            If true, this value prompts the user to accept the license when
            installing the package. 
        -->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <!-- 
            The description can be used in package manager UI. Note that the
            nuget.org gallery uses information you add in the portal. 
        -->
    <description>Contains contracts and generic behavior that is available to the entire eco-system.</description>

    <!-- Copyright information -->
    <copyright>Copyright ©2019 Aetos Consulting</copyright>

    <!-- Tags appear in the gallery and can be used for tag searches -->
    <tags>gis interfaces extensions</tags>

    <!-- Dependencies are automatically installed when the package is installed -->
    <dependencies>
      <dependency id="Newtonsoft.Json" version="12.0.1" />
    </dependencies>
  </metadata>

  <!-- A readme.txt to display when the package is installed 
  <files>
    <file src="readme.txt" target="" />
  </files>-->
  </package>
